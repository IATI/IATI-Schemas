<?xml version="1.0"?>

<iati-organisations version="xx">

  <iati-organisation>
    <organisation-identifier>12345</organisation-identifier>
    <name>
      <narrative>Name here</narrative>
    </name>
    <reporting-org type="xx" ref="xx">
      <narrative>Organisation name</narrative>
    </reporting-org>
    
    <!--minimal example-->
    <document-link url="http://www.example.org/" format="text/html">
      <title> 
        <narrative>Document title</narrative> <!-- should be at least 1 --> 
      </title>
      <category code="XXX"/>
    </document-link>
    
    <!-- full example -->
    <document-link url="http://www.example.org/" format="text/html">
      <title> 
        <narrative>Document title</narrative>
        <narrative xml:lang="fr">Document title</narrative>
      </title>
      <category code="XXX"/>
      <category code="yyy"/>
      <language code="en"/>
      <document-date iso-date="2013-10-05"/>
      <recipient-country code="xX"></recipient-country>
      <recipient-country code="xX"></recipient-country>
    </document-link>
    
  </iati-organisation>

</iati-organisations>
