<!--
  Test that schema accepts top-level extended elements with an
  explicit XML namespace
-->
<iati-organisations version="xx">

  <iati-organisation>
    <organisation-identifier>12345</organisation-identifier>
    <!-- extended foo element allowed with an XML namespace -->
    <foo xmlns="http://www.example.org/">yyy</foo>
  </iati-organisation>

</iati-organisations>
