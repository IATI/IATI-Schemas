<?xml version="1.0"?>

<!-- Test that the schema rejects currency values with a decimal part -->
<iati-organisation>
  <iati-identifier>12345</iati-identifier>
  <total-budget>
    <!-- currency must be a number -->
    <value value-date="2010-07-07">A100.00</value>
  </total-budget>
</iati-organisation>