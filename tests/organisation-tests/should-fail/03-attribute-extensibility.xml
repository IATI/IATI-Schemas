<?xml version="1.0"?>

<!-- Test that the schema rejects extended attributes not in 
  an XML namespace -->


<iati-organisations version="xx">

  <iati-organisation>
    <organisation-identifier>12345</organisation-identifier>
    <!-- @foo not allowed, since it's not in a namespace -->
    <name foo="yyy">
      <narrative>Name here</narrative>
    </name>
    <reporting-org type="xx" ref="xx">
      <narrative>Organisation name</narrative>
    </reporting-org>
    
  </iati-organisation>
</iati-organisations>
