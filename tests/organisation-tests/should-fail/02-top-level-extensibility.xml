<?xml version="1.0"?>

<!--
  Test that schema rejects top-level extended elements without an
  explicit XML namespace
-->
<iati-organisations version="xx">

  <iati-organisation>
    <organisation-identifier>12345</organisation-identifier>
    <name>
      <narrative>Name here</narrative>
    </name>
    <reporting-org type="xx" ref="xx">
      <narrative>Organisation name</narrative>
    </reporting-org>
    
      <!-- extended foo element not allowed without an XML namespace -->
      <foo>yyy</foo>
  </iati-organisation>

</iati-organisations>
