<?xml version="1.0"?>

<!--
  Test that the schema rejects adding a new element that's not in a 
  namespace into mixed content.
-->
<!-- 
  This is not actually mixed content any more but should still fail
-->
<iati-activities version="xx">
  <iati-activity version="xx">
    <iati-identifier>12345</iati-identifier>
    <!-- shouldn' allow foo element -->
    <description>xxx <foo>yyy</foo> zzz</description>
  </iati-activity>
</iati-activities>
