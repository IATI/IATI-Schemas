<?xml version="1.0"?>

<!--
  Test that currency values reject decimals.
-->
<iati-activities>
  <iati-activity>
    <iati-identifier>12345</iati-identifier>
    <!-- must be a number -->
    <budget>
      <value value-date="2013-01-01">A999.99</value>
    </budget>
  </iati-activity>
</iati-activities>