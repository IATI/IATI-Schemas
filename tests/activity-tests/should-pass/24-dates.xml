<?xml version="1.0"?>

<!--
  Test different date variations
-->
<iati-activities>
  <iati-activity>
    <iati-identifier>12345</iati-identifier>
    <transaction>
      <!-- YYYY-MM-DD -->
      <transaction-date iso-date="2010-07-07">July 7, 2010</transaction-date>
    </transaction>
    <transaction>
      <!-- YYYY-MM -->
      <transaction-date iso-date="2010-07">July, 2010</transaction-date>
    </transaction>
    <transaction>
      <!-- YYYY -->
      <transaction-date iso-date="2010">2010</transaction-date>
    </transaction>
    <transaction>
      <!-- YYYY-MM-DD'T'hh:mm:ss-zz:zz -->
      <transaction-date iso-date="2010-07-07T12:00:00-04:00">July 7, 2010 @ 12:00:00 EDT</transaction-date>
    </transaction>
  </iati-activity>
</iati-activities>