<?xml version="1.0"?>

<!-- every place @type is allowed -->
<iati-activities version="xx">
  <iati-activity> <!-- at least 1 -->
    <iati-identifier></iati-identifier>  <!-- 1 and only 1-->
    <reporting-org type="xx" ref="xx"><narrative>Organisation name</narrative></reporting-org> <!-- 1 and only 1-->
    <title> <!-- 1 and only 1-->
      <narrative>Xxxxxxx</narrative> <!-- At least 1-->
    </title>
    <description type="X">
      <narrative>Xxxxxxx</narrative> <!-- At least 1-->
    </description>
    <participating-org type="X" role="xx"></participating-org> <!-- At least 1-->
    <!--<other-identifier></other-identifier>-->
    <activity-status code="xx"/> <!-- 1 and only 1-->
    <activity-date type="xx" iso-date="2013-11-27"/><!-- At least 1 --> <!--Narative allowed-->
    
    <contact-info type="xxx" />
    
    <budget type="X">
      <period-start iso-date="2011-01-01" />
      <period-end iso-date="2011-12-31" />
      <value value-date="2011-01-01" currency="GBP">1000000</value>
    </budget>

    <planned-disbursement type="x">
      <period-start iso-date="2011-01-01" />
      <!--<period-end iso-date="2011-01-01" />--><!-- optional -->
      <value value-date="2011-01-01" currency="GBP">500000</value>
    </planned-disbursement>

    <related-activity ref="X" type="Y"/>
    
    <conditions attached="1">
      <condition type="1">
        <narrative>[some text here]</narrative>
      </condition>
    </conditions> 
    
    <result type="X">
      <title>
        <narrative>Treatment: timely detection and quality treatment of cases</narrative>
      </title>
      <indicator measure="Unit" ascending="true">
        <title>
          <narrative>Number of new smear positive cases detected under DOTS. (Case detection rate under DOTS)</narrative>
        </title>
      </indicator>
    </result>
    

  </iati-activity>
</iati-activities>
